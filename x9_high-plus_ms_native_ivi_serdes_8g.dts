&i2c6 {
        status = "okay";
		smiacc:smiacc@18 {
			compatible = "SMI230ACC";
			reg = <0x18>;
			//ACC_INT -> GPIO.IO67 GPIO_D3
			gpio_irq = <&port4c 3 GPIO_ACTIVE_HIGH>;
							status = "okay";
		};
		smigyro:smiacc@68 {
			compatible = "SMI230GYRO";
			reg = <0x68>;
			//GYRO_INT -> GPIO.IO61 GPIO_C13
							gpio_irq = <&port4b 29 GPIO_ACTIVE_HIGH>;
							status = "okay"
		}
};

